 $(document).ready(function() {
 	$("#requestany").click(function() {
		if(this.checked){
		$("input[@name=checkitem]").each(function() {
			$(this).attr("checked","");
		});
		}
	});
 	$("input[@name=checkitem]").click(function() {
		onechecked = 0;
		$("input[@name=checkitem]").each(function() {
			if(this.checked){
				onechecked = 1;
			}
		});
		if(onechecked == 1){
			$("#requestany").attr("checked","");
		} else {
			$("#requestany").attr("checked","checked");
		}
	});
 });
