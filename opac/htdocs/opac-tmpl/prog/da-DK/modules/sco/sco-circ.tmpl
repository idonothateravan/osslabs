<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>Kohas selvbetjente udlån</title>
<meta http-equiv="Content-Type" content="text/html; tegnsæt=iso-8859-1">
<link rel="stylesheet" type="text/css" href="<!-- TMPL_VAR name="themelang" -->/css/sco.css" />
<script type="text/javascript" language="JavaScript">
    <!-- Hide from old browsers
    function openWindow(targeturl, windowname) {
        newwin =
        window.open(targeturl,windowname,"height=480,width=640,scrollbars,resizable")
    }
    function focusOnFirst (){
        document.forms[1].elements[0].focus();
    }

    function abort(){
        window.open("/cgi-bin/koha/sco/abort.pl","aborting","height=5,width=5");
    }
    // end hiding -->
</script>
</head>
<body onload="focusOnFirst();" onblur="self.focus();">

<!-- TMPL_IF name="loggedinusername" -->
<h3><!-- TMPL_VAR NAME="libraryname" --> System til selvbetjent udlån</h3>

<div id="main">
<div class="form">
<!-- TMPL_IF NAME="dbarred" -->
<div class="error"><h3>Beklager, udlån kan ikke udføres</h3>
There is a problem with your account.  Please ask for help at the circulation desk.
<div id="abort"><form method="get" action="#">
<input type="submit" value="Klik her for at afslutte" class="complete" onclick="self.close();"></form></div>
</div>
<!-- /TMPL_IF -->

<!-- TMPL_IF name="confirm" -->
	<div class="error">
		<h3>Advarsel:</h3>
		<p>Prøver at låne <!-- TMPL_VAR name="title" --> <br />
			til <!-- TMPL_VAR name="firstname" --> <!-- TMPL_VAR name="surname" -->.</p>
		<p><!-- TMPL_VAR name="confirm" --></p>
		<form action="/cgi-bin/koha/sco/sco-circ.pl" method="get">
		  <input type="hidden" name="borrnumber" value="<!-- TMPL_VAR name="borrowernumber" -->" />
		  <input type="hidden" name="barcode" value="<!-- TMPL_VAR name="barcode" -->" />
		  <input type="button" name="confirmed" value="Bekræfte?">
		  <input type="button" name="cancel" value="Afbryd">
		</form>
	</div>
<!-- /TMPL_IF -->

<!-- TMPL_IF name="impossible" -->
	<div class="error">
		<h3>Fejl ved bestilling af bog</h3>
		<!-- TMPL_VAR name="impossible" -->
	</div>
<!-- /TMPL_IF -->

<!-- TMPL_IF name="message" -->
	<div class="error">
		<h3>Ingen lån passet med <!-- TMPL_VAR name="message" --></h3>
	</div>
<!-- /TMPL_IF -->

<!-- TMPL_UNLESS NAME="dbarred" -->
<div id="complete">
<form method="get" action="/cgi-bin/koha/sco/abort.pl"><input type="submit" value="Klik her for at afslutte transaktionen" class="complete" onclick="self.close();">
</form>
</div>
<form method="post" action="/cgi-bin/koha/sco/sco-circ.pl">
<h4>Lånt ut til <!-- TMPL_VAR name="firstname" --> <!-- TMPL_VAR name="surname" --> (<!-- TMPL_VAR name="cardnumber" -->)</h4>
<label for="barcode">Skan et nyt udlån eller opgiv stregkoden:</label> 
<input id="barcode" name="barcode" size="14" maxlength="14" class="unfocus" onfocus="this.className='focus';" onblur="this.className='unfocus';" /> 
<input type="submit" value="Indsend" class="submit">
<input type="hidden" name="borrnumber" value="<!-- TMPL_VAR name="borrowernumber" -->" />
</form>
<!-- /TMPL_UNLESS -->
<!-- /TMPL_IF -->

<div id="borrowerdetails">
<!-- TMPL_IF NAME="issues_count" -->
<div class="table"><table><caption>Numre <span class="count">(<!-- TMPL_VAR NAME="issues_count" --> totalt)</span></caption>
<!-- ISSUES TABLE ROWS -->
<tr><th colspan="2">Titel</th><th>Hyldeplads</th><th>Utlånt til</th><th>Forlæng</th><th>Bøder</th></tr>
<!-- TMPL_LOOP NAME="ISSUES" -->
<!-- TMPL_IF NAME="overdue" --><tr class="overdue"><!-- TMPL_ELSE --><tr><!-- /TMPL_IF -->
<td><!-- TMPL_IF NAME="itemtypeimages" --> <img src="<!-- TMPL_VAR NAME="themelang" -->/images/<!-- TMPL_VAR NAME="itemtype" -->.gif" alt="<!-- TMPL_VAR NAME="description" -->" title="<!-- TMPL_VAR NAME="description" -->" /><!-- /TMPL_IF --></td>
<td><!-- TMPL_IF NAME="amazonimages" --> <!-- TMPL_IF NAME="isbn" --><a href="/cgi-bin/koha/opac-detail.pl?bib=<!-- TMPL_VAR NAME="biblionumber" ESCAPE="URL" -->"><img src="http://images.amazon.com/images/P/<!-- TMPL_VAR name="isbn" -->.01.THUMBZZZ.jpg" alt="Billede af omslaget" class="thumbnail"></a><!-- /TMPL_IF --> <!-- /TMPL_IF --><a href="/cgi-bin/koha/opac-detail.pl?bib=<!-- TMPL_VAR NAME="biblionumber" -->"><!-- TMPL_VAR NAME="title" --></a><span class="item-details">
<!-- TMPL_VAR NAME="author" --></span></td>
<td><!-- TMPL_VAR NAME="classification" --></td>
<!-- TMPL_IF NAME="overdue" --><td class="overdue"><!-- TMPL_VAR NAME="date_due" --></td><!-- TMPL_ELSE --><td><!-- TMPL_VAR NAME="date_due" --></td><!-- /TMPL_IF -->
<td>
<!-- TMPL_IF NAME="status" -->
<a href="/cgi-bin/koha/opac-renew.pl?from=opac_user&amp;item=<!-- TMPL_VAR NAME="itemnumber" -->&amp;bornum=<!-- TMPL_VAR NAME="borrowernumber" -->">Forlæng</a>
<!-- TMPL_ELSE -->
Kan ikke forlænges<!-- /TMPL_IF -->
</td>
<td><!-- TMPL_IF NAME="charges" -->Ja<!-- TMPL_ELSE -->Nej<!-- /TMPL_IF --></td></tr>
<!-- /TMPL_LOOP -->
</table>
</div>
</div> <!-- borrowerdetails -->

</div>
<!-- /TMPL_IF -->
</body>
</html>
