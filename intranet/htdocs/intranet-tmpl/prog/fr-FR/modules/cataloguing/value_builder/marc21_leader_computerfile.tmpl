			<!-- /TMPL_IF -->

			<!-- TMPL_IF name="f5c" -->
				<option value="c" SELECTED>c- Corrigé ou révisé</option>
			<!-- TMPL_ELSE -->
				<option value="c">c- Corrigé ou révisé</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f5d" -->
				<option value="d" SELECTED>d- supprimé</option>
