<tr><th scope="row"><label for="author">Verfasser:</label></th><td><input name="author" id="author" type="text" size="40" /></td>
</tr>
<tr><th scope="row"><label for="seriestitle">Serientitel:</label><br />
<i>(wenn anwendbar)</i></th><td><input name="seriestitle" type="text" id="seriestitle" size="40" /></td>
</tr>
<tr><th scope="row"><label for="copyrightdate">Copyrightdatum:</label></th><td><input id="copyrightdate" name="copyrightdate" type="text" size="40" /></td>
</tr>
<tr><th scope="row"><label for="abstract">Kurzfassung:</label></th><td><textarea cols="30" id="abstract" rows="6" name="abstract"></textarea></td>
</tr>
<tr><th scope="row"><label for="bib-notes">Hinweise:</label></th><td><textarea cols="30" rows="6" id="bib-notes" name="notes"></textarea></td>
</tr>
</table>
<input type="submit" value="Datensatz hinzufügen">

</form>
