<form action="savebiblio.pl" method="post">

<table>
	<caption>Einen neuen bibliographischen Datensatz hinzufügend</caption>
	<tr><th scope="row"><label for="title">Titel: *</label></th><td><input name="title" id="title" type="text" size="40" /> * erforderlich</td>
</tr>
<tr><th scope="row"><label for="subtitle">Zusatz zum Titel:</label></th><td><input name="subtitle" id="subtitle" type="text" size="40" /></td>
